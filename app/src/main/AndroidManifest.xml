<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.niharikakhanna.app.quiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".QuestionDatabase">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".LoginSuccessActivity">
        </activity>
        <activity android:name=".GameActivity">
        </activity>
        <activity android:name=".QuizActivity">
        </activity>
        <activity android:name=".ScoreDisplay">
        </activity>
        <activity android:name=".MultiplayerQuiz">
        </activity>
        <activity android:name=".MultiplayerStart">
        </activity>
        <activity android:name=".ScoreDisplayMulti">
        </activity>
    </application>

</manifest>